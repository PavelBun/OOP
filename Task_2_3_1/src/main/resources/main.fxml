<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="org.example.GameController">
    <Canvas fx:id="gameCanvas" width="340" height="300" />
    <Text fx:id="startText" text="Нажмите ПРОБЕЛ для старта!"
          style="-fx-font-size: 24; -fx-fill: white; -fx-effect: dropshadow(gaussian, black, 2, 0.5, 0, 0)"/>
</StackPane>